    },
    "files": [
        "build"
    ],
    "devDependencies": {
        "@jest/test-sequencer": "^28.1.1",
        "@types/jest": "^28.1.4",
        "@types/node": "^18.0.3",
        "@typescript-eslint/eslint-plugin": "^5.30.5",
        "@typescript-eslint/parser": "^5.30.5",
        "auto-changelog": "^2.4.0",
        "eslint": "^8.19.0",
        "fs-extra": "^10.1.0",
        "jest": "^28.1.2",
        "release-it": "^15.1.1",
        "ts-jest": "^28.0.5"
    }
}
