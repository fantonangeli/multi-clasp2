    "files": [
        "build"
    ],
    "devDependencies": {
        "@jest/test-sequencer": "^29.5.0",
        "@types/jest": "^29.4.0",
        "@types/node": "^18.15.0",
        "@typescript-eslint/eslint-plugin": "^5.54.1",
        "@typescript-eslint/parser": "^5.54.1",
        "auto-changelog": "^2.4.0",
        "eslint": "^8.35.0",
        "fs-extra": "^11.1.0",
        "jest": "^29.5.0",
        "release-it": "^15.8.0",
        "ts-jest": "^29.0.5"
    }
